{
    "name": "Battery Alert",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Display the notification of Batter Status",
    "permissions": [
        "notifications",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "128": "icon.png"
     },
    "background": {
        "scripts": ["index.js"],
        "persistent": true
    }
}